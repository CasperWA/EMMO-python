    <!-- http://emmc.info/emmo-math#EMMO_ae15fb4f_8e4d_41de_a0f9_3997f89ba6a2 -->

    <owl:Class rdf:about="http://emmc.info/emmo-math#EMMO_ae15fb4f_8e4d_41de_a0f9_3997f89ba6a2">
        <rdfs:subClassOf rdf:resource="http://emmc.info/emmo-math#EMMO_1eed0732_e3f1_4b2c_a9c4_b4e75eeb5895"/>
        <emmo-core:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9>A &apos;varaible&apos; that stand for a well known constant.</emmo-core:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9>
