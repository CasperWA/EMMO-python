    <!-- http://emmc.info/emmo-core#EMMO_517dfaf9_4970_41ac_81ee_d031627d2c7c -->

    <owl:ObjectProperty rdf:about="http://emmc.info/emmo-core#EMMO_517dfaf9_4970_41ac_81ee_d031627d2c7c">
        <rdfs:subPropertyOf rdf:resource="http://emmc.info/emmo-core#EMMO_03212fd7_abfd_4828_9c8e_62c293052d4b"/>
        <rdfs:subPropertyOf>
