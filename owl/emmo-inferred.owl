        </rdfs:subPropertyOf>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <emmo-core:EMMO_70fe84ff_99b6_4206_a9fc_9a8931836d84>Exy ≡ ∀z(Czx → Czy)</emmo-core:EMMO_70fe84ff_99b6_4206_a9fc_9a8931836d84>
        <rdfs:comment>Enclosure is reflexive and transitive.</rdfs:comment>
        <rdfs:label xml:lang="en">encloses</rdfs:label>
        <rdfs:seeAlso>is_enclosed_by</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- http://emmc.info/emmo-core#EMMO_9380ab64_0363_4804_b13f_3a8a94119a76 -->

    <owl:ObjectProperty rdf:about="http://emmc.info/emmo-core#EMMO_9380ab64_0363_4804_b13f_3a8a94119a76">
