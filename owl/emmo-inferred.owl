        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <emmo-core:EMMO_70fe84ff_99b6_4206_a9fc_9a8931836d84>PPxy ≡ Pxy ∧ ¬(x=y)</emmo-core:EMMO_70fe84ff_99b6_4206_a9fc_9a8931836d84>
        <rdfs:label xml:lang="en">has_proper_part</rdfs:label>
        <rdfs:seeAlso>is_proper_part_of</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- http://emmc.info/emmo-core#EMMO_9cb984ca_48ad_4864_b09e_50d3fff19420 -->

    <owl:ObjectProperty rdf:about="http://emmc.info/emmo-core#EMMO_9cb984ca_48ad_4864_b09e_50d3fff19420">
        <rdfs:subPropertyOf rdf:resource="http://emmc.info/emmo-core#EMMO_d893d373_b579_4867_841e_1c2b31a8d2c6"/>
