    <!-- http://emmc.info/emmo-core#EMMO_d893d373_b579_4867_841e_1c2b31a8d2c6 -->

    <owl:ObjectProperty rdf:about="http://emmc.info/emmo-core#EMMO_d893d373_b579_4867_841e_1c2b31a8d2c6">
        <rdfs:subPropertyOf rdf:resource="http://emmc.info/emmo-core#EMMO_6703954e_34c4_4a15_a9e7_f313760ae1a8"/>
